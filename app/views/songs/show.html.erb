<h1>Song: <%= @song.name %></h1>
<h2>Albun: <%= @album.name %></h2>
<h3>Lyrics: <%= @song.lyrics %></h3>
<%= button_to "Edit Song",
              edit_album_song_path(@album, @song),
              method: :get %>
<%= button_to "Delete Song",
              album_song_path(@album, @song),
              method: :delete %>
<%= button_to "Return to Album", 
              album_path(@album),
              method: :get %>
